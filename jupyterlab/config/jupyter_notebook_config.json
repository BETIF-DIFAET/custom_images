{  
"NotebookApp": {
    "nbserver_extensions": {
      "rucio_jupyterlab.server": true
    }
  },
        "RucioConfig": {
            "default_instance": "escape",
            "default_auth_type": "x509_proxy",
            "instances": [
                {
                        "name": "cms",
                        "display_name": "CMS RUCIO",
                        "rucio_base_url": "http://cms-rucio.cern.ch",
                        "rucio_auth_url": "https://cms-rucio-auth.cern.ch",
                        "rucio_webui_url": "https://cms-rucio-webui.cern.ch",
                        "rucio_ca_cert": "/cvmfs/grid.cern.ch/etc/grid-security/certificates/",
                        "vo": "cms",
                        "voms_enabled": true,
                        "voms_certdir_path": "/cvmfs/grid.cern.ch/etc/grid-security/certificates/",
                        "voms_vomsdir_path": "/cvmfs/grid.cern.ch/etc/grid-security/vomsdir/",
                        "voms_vomses_path": "/cvmfs/grid.cern.ch/etc/grid-security/vomses/",
                        "mode": "download",
                        "wildcard_enabled": true
                },
                {
                        "name": "escape",
                        "display_name": "ESCAPE RUCIO",
                        "rucio_base_url": "https://vre-rucio.cern.ch",
                        "rucio_auth_url": "https://vre-rucio-auth.cern.ch",
                        "rucio_webui_url": "https://vre-rucio-ui.cern.ch",
                        "rucio_ca_cert": "/cvmfs/grid.cern.ch/etc/grid-security/certificates/",
                        "vo": "escape",
                        "voms_enabled": true,
                        "voms_certdir_path": "/cvmfs/grid.cern.ch/etc/grid-security/certificates/",
                        "voms_vomsdir_path": "/cvmfs/grid.cern.ch/etc/grid-security/vomsdir/",
                        "voms_vomses_path": "/cvmfs/grid.cern.ch/etc/grid-security/vomses/",
                        "mode": "download",
                        "wildcard_enabled": true
                }
            ]
    }
}